<nav id="subnav_map" class="navbar navbar-inverse">
	<ul class="nav">
		<li{{^country}} class="active" {{/country}}><a href="/map/countries" title="Show all countries">All</a></li>
		<li class="space"></li>
		{{#countries}}
			<li{{#active}} class="active" {{/active}}><a href="/map/countries/{{id}}" title="{{name}}">{{id}}</a></li>{{/countries}}
	</ul>
</nav>
<div id="main-container">
	<div id="main" role="main">
		<div class="row">
			{{^country}}
				<div class="span6 info_box">
					<h4>Overview Countries</h4>

					<div class="mep_map">
						{{#overview_countries}}{{>countries_map}}{{/overview_countries}}
					</div>
				</div>
				<div class="span6 info_box">
					<h4>Overview Europe</h4>

					<div class="overview overview_height_large">
						<div class="overview_text_box">
							{{#overview}}
								<div class="overview_text">
									Amendments: {{total}}<br/>
									<span class="legend_strong">...stronger: {{pro}}</span><br/>
									<span class="legend_weak">...weaker: {{contra}}</span><br/>
									<span class="legend_neutral">...neutral: {{neutral}}</span><br/>
								</div>
							{{/overview}}
							{{^disable_email}}
								<div class="btn_email">
									<a class="btn btn-primary btn-small" href="#">Send E-Mail to all <span
											title="{{group.long}}">{{group.short}}</span>
										MEPs</a>
								</div>
							{{/disable_email}}
						</div>
						<div class="overview_chart">
							<div class="bigpie">
								{{#overview}}{{>pie}}{{/overview}}
							</div>
						</div>
					</div>
				</div>
			{{/country}}
			{{#country}}
				<div class="span6 info_box">
					<div class="overview_bar_box">
						<h4>Overview Groups in {{name}}</h4>

						<div class="mep_bars">
							{{#group_overview}}{{>group_bars}}{{/group_overview}}
						</div>
					</div>
				</div>
				<div class="span6 info_box">
					<h4>Overview {{name}}</h4>

					<div class="overview overview_height_large">
						<div class="overview_text_box">
							{{#overview}}
								<div class="overview_text">
									Amendments: {{total}}<br/>
									<span class="legend_strong">...stronger: {{pro}}</span><br/>
									<span class="legend_weak">...weaker: {{contra}}</span><br/>
									<span class="legend_neutral">...neutral: {{neutral}}</span><br/>
								</div>
							{{/overview}}
							{{^disable_email}}
								<div class="btn_email">
									<a class="btn btn-primary btn-small" href="#">Send E-Mail to all <span
											title="{{group.long}}">{{group.short}}</span>
										MEPs</a>
								</div>
							{{/disable_email}}
						</div>
						<div class="overview_chart">
							<div class="bigpie">
								{{#overview}}{{>pie}}{{/overview}}
							</div>
						</div>
					</div>
				</div>
			{{/country}}

		</div>

		<div class="row">
			<div class="span12">
				<section class="info">
					Kurzer Erklärbärtext Kurzer Erklärbärtext Kurzer Erklärbärtext Kurzer Erklärbärtext Kurzer
					Erklärbärtext
					Kurzer
					Erklärbärtext Kurzer
					Erklärbärtext Kurzer Erklärbärtext Kurzer Erklärbärtext Kurzer Erklärbärtext Kurzer
					Erklärbärtext
					Kurzer
					Erklärbärtext Kurzer Erklärbärtext
					Kurzer Erklärbärtext Kurzer Erklärbärtext Kurzer Erklärbärtext
				</section>
			</div>
		</div>
		<div class="row">
			{{#flops.length}}
				<div class="span6 {{^tops.length}}spanpadding3 {{/tops.length}}info_box border_weaker">
					<h4 class="background_weaker info_box_head">Top {{flops.length}} for less Data Privacy in {{^country}}Europe{{/country}}{{#country}}{{name}}{{/country}}</h4>

					<div class="flop_box">{{#flops}}{{>mep_line}}{{/flops}}</div>
				</div>
			{{/flops.length}}
			{{#tops.length}}
				<div class="span6 {{^flops.length}}spanpadding3 {{/flops.length}}info_box border_stronger">
					<h4 class="background_stronger info_box_head">Top {{tops.length}} for more Data Privacy in {{^country}}
						Europe{{/country}}{{#country}}{{name}}{{/country}}</h4>

					<div class="top_box">{{#tops}}{{>mep_line}}{{/tops}}</div>
				</div>
			{{/tops.length}}
		</div>
	</div>
</div>
